  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "yarn server",
    "prod": "yarn build && yarn server:prod",
    "server": "cross-env NODE_ENV=development nodemon -e 'ts, tsx, js, jsx, mjs, cjs, json' --watch src/app/server.ts --exec 'yarn node --no-warnings --loader ts-node/esm src/app/server.ts'",
    "server:prod": "cross-env NODE_ENV=production nodemon --exec 'yarn node --no-warnings --loader ts-node/esm src/app/server.ts'",
    "lint": "eslint src --resolve-plugins-relative-to \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint src --resolve-plugins-relative-to \"./src/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "moduleRoots": [
    "node_modules",
    "src"
  ],
